<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label styleClass="panel-title" text="Maps">
         <VBox.margin>
            <Insets left="8.0" top="8.0" />
         </VBox.margin>
      </Label>
      <SplitPane dividerPositions="0.75" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <items>
            <StackPane fx:id="stackPane" prefHeight="150.0" prefWidth="200.0" />
            <HBox prefHeight="100.0" prefWidth="200.0" />
         </items>
      </SplitPane>
   </children>
   <stylesheets>
      <URL value="@../styles.css" />
      <URL value="@../icons_styles.css" />
   </stylesheets>
</VBox>
